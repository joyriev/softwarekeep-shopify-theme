{% comment %} Convert the non product currencies local users exchange rate {% endcomment %}
<script>
  document.addEventListener('DOMContentLoaded', function() {
    const changeablePrices = document.querySelectorAll('.price_to_modify');
    changeablePrices.forEach(price => {
      const priceText = price.textContent;
      const priceNumber = parseFloat(priceText.replace('$', ''));
      const convertedPrice = priceNumber * Shopify.currency.rate;
      price.textContent = `{{ cart.currency.symbol }}${convertedPrice.toFixed(2)}`;
    });
  });
</script>

