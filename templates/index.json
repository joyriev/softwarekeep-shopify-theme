        "text_taYMMN": {
          "type": "text",
          "settings": {
            "iconImage": "shopify://shop_images/Faces_Testimonials-03.png",
            "test_text": "<p>SoftwareKeep‚Äôs combo deals and 24/7 expert service enabled us to scale efficiently, enhance our security, and achieve impressive ROI. They offer solutions that drive both immediate and long-term results.</p>",
            "authorName": "Tech Source",
