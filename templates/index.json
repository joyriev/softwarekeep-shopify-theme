        "text_4g98wB": {
          "type": "text",
          "settings": {
            "iconImage": "shopify://shop_images/Faces_Testimonials-01.png",
            "test_text": "<p>As our business evolved, SoftwareKeep became an invaluable partner. Their personalized, expert 24/7 support outshines larger providers, helping us save costs while receiving far better service.</p>",
            "authorName": "TechFlow",
